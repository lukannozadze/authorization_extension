{
  "name": "Authorization Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "host_permissions": [
    "https://*.accounts.google.com/",
    "https://*.living-aardvark-75.clerk.accounts.dev/"
  ],
  "permissions": [
    "storage",
    "identity",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "auth.png",
    "48": "auth.png",
    "128": "auth.png"
  }
}
